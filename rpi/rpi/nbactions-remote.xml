<?xml version="1.0" encoding="UTF-8"?>
<actions>
  <action>
    <actionName>run</actionName>
    <packagings>
      <packaging>jar</packaging>
    </packagings>
    <goals>
      <goal>process-classes</goal>
      <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
    </goals>
    <properties>
      <exec.args>-classpath %classpath at.or.reder.rpi.Main</exec.args>
      <exec.executable>java</exec.executable>
    </properties>
  </action>
  <action>
    <actionName>debug</actionName>
    <packagings>
      <packaging>jar</packaging>
    </packagings>
    <goals>
      <goal>process-classes</goal>
      <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
    </goals>
    <properties>

      <exec.args>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath at.or.reder.rpi.Main</exec.args>
      <jpda.listen>true</jpda.listen>
      <exec.executable>java</exec.executable>
    </properties>
  </action>
</actions>
